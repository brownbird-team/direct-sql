{
	"name": "panda-sql",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"db:migrate:dev": "NODE_ENV=development npx sequelize db:migrate",
		"db:migrate:prod": "NODE_ENV=production npx sequelize db:migrate",
		"db:refresh:dev": "NODE_ENV=development npx sequelize db:seed:undo:all && npx sequelize db:migrate:undo:all && npx sequelize db:migrate && npx sequelize db:seed:all",
